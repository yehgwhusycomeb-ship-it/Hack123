    local RaycastResult = WorkspaceService:Raycast(FunctionParameter + Vector3.new(0, 50, 0), Vector3.new(0, -100, 0), RaycastParams)

    if RaycastResult then
        return RaycastResult.Position + Vector3.new(0, 5, 0)
    else
        return FunctionParameter
    end
end
