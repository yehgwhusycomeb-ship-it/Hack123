local EmptyTable = {}

SettingsTab:AddSwitch('Highlight Barriers', function(CallbackParameter)
    FalseValue = CallbackParameter

    if CallbackParameter then
        local DescendantIndex, DescendantKey, DescendantValue = pairs(game.Workspace:GetDescendants())

        while true do
            local UnusedVariable

            DescendantValue, UnusedVariable = DescendantIndex(DescendantKey, DescendantValue)

            if DescendantValue == nil then
                break
            end
            if UnusedVariable:IsA('BasePart') and (UnusedVariable.Name ~= 'Baseplate' and (not UnusedVariable:IsDescendantOf(game.Players.LocalPlayer.Character) and UnusedVariable.Transparency == 1)) then
                UnusedVariable.Transparency = 0
                UnusedVariable.Color = Color3.fromRGB(0, 0, 255)

                local HighlightInstance = Instance.new('Highlight')

                HighlightInstance.FillColor = Color3.fromRGB(0, 0, 255)
                HighlightInstance.OutlineColor = Color3.fromRGB(0, 0, 128)
                HighlightInstance.FillTransparency = 0
                HighlightInstance.OutlineTransparency = 0
                HighlightInstance.Parent = UnusedVariable

                table.insert(EmptyTable, {
                    part = UnusedVariable,
                    highlight = HighlightInstance,
                })
            end
        end
    else
        local TableKey, TableValue, TableIndex = pairs(EmptyTable)

        while true do
            local PartContainer

            TableIndex, PartContainer = TableKey(TableValue, TableIndex)

            if TableIndex == nil then
                break
            end

            local PartInstance = PartContainer.part
            local HighlightObject = PartContainer.highlight

            if PartInstance and (PartInstance.Parent and HighlightObject) then
                PartInstance.Transparency = 1
                PartInstance.Color = PartInstance.Color

                HighlightObject:Destroy()
            end
        end

        EmptyTable = {}
    end
end)

