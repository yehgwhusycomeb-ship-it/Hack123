                        CharacterContainer.CanCollide = false
                    end
                end
            end
        end)
    else
        if NilValue then
            NilValue:Disconnect()

            NilValue = nil
        end
        if LocalPlayerObject.Character then
            local CharacterValue, CharacterProperty, CharacterAttribute = pairs(LocalPlayerObject.Character:GetDescendants())

            while true do
                local UnusedReference

                CharacterAttribute, UnusedReference = CharacterValue(CharacterProperty, CharacterAttribute)

